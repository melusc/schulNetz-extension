{
  "author": "",
  "dependencies": {
    "preact": "^10.5.13",
    "preact-router": "^3.2.1"
  },
  "description": "",
  "devDependencies": {
    "@types/chrome": "^0.0.141",
    "concurrently": "^6.1.0",
    "copy-webpack-plugin": "^8.1.1",
    "env-cmd": "^10.1.0",
    "prettier": "^2.3.0",
    "terser-webpack-plugin": "^5.1.2",
    "ts-loader": "^9.2.1",
    "typescript": "^4.2.4",
    "web-ext": "^6.1.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "xo": "^0.40.1"
  },
  "license": "ISC",
  "main": "index.js",
  "name": "schulnetz_extension",
  "scripts": {
    "build": "npm run lint && npm run webpack && web-ext lint -s dist",
    "lint": "prettier --config .prettierrc -w \"{*,**/*}.{js,ts,tsx}\" && xo --fix",
    "sign": "env-cmd \"web-ext sign -s dist\"",
    "start": "concurrently \"web-ext run -s dist\" \"npm:webpack:w\"",
    "test": "xo",
    "webpack": "webpack -c production.config.js",
    "webpack:w": "webpack -c production.config.js -w"
  },
  "sideEffects": false,
  "version": "1.0.0",
  "xo": {
    "overrides": [
      {
        "files": "src/**/*.{ts,tsx}",
        "rules": {
          "@typescript-eslint/no-unused-vars": [
            "error",
            {
              "varsIgnorePattern": "Fragment"
            }
          ]
        }
      },
      {
        "files": "./*.js",
        "rules": {
          "unicorn/prefer-module": "off"
        }
      }
    ],
    "space": true
  }
}
